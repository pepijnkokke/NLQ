    private
      go : ∀ {I J B C}
         → (I⁻ : Polarised - I) (f : LG I [ B ⇚ C ]ᴶ)
         → {J⁻ : Polarised - J} (g : ∀ {G} → LG I [ G ]ᴶ ⋯ J [ G ]ᴶ)
         → Origin J⁻ (g [ f ]ᴰ)
      go I⁻ f {J⁻} g with viewOrigin I⁻ f
      ... | origin h₁ h₂ f′ pr = origin h₁ h₂ (g < f′ >ᴰ) pr′
        where
          pr′ : g [ f ]ᴰ ≡ (g < f′ >ᴰ) [ m⇚ h₁ h₂ ]ᴰ
          pr′ rewrite <>ᴰ-def f′ g (m⇚ h₁ h₂) = cong (_[_]ᴰ g) pr



