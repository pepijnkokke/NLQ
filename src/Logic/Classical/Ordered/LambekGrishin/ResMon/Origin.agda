    viewOrigin (._ ⊢> (A <⊕ B)) (r₁¹ f)     = go (₁> (A <⊕ B) <⊢ _)     f  (r₁¹ [])
    viewOrigin (._ ⊢> (A <⇒ B)) (r¹₁ f)     = go ((A <⇒ B) <¹ <⊢ _)     f  (r¹₁ [])
    viewOrigin (._ ⊢> (A <⇒ B)) (r₁¹ f)     = go (₁> (A <⇒ B) <⊢ _)     f  (r₁¹ [])
