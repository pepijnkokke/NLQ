module el where

  data Origin {J B} (J⁺ : Polarised + J) (f : LG J [ el B ]ᴶ) : Set ℓ where
       origin : (f′ : ∀ {G} → LG G ⊢ el B ⋯ J [ G ]ᴶ)
              → (pr : f ≡ f′ [ ax ]ᴰ)
              → Origin J⁺ f


  mutual
