    viewOrigin (._ ⊢> (B <⁰))   (r□◇ f)     = go (_ ⊢> □> (B <⁰))       f  (r□◇ [])
    viewOrigin (._ ⊢> (B <⁰))   (m⁰  f)     = go (B <⊢ _)               f  (m⁰ [])
    viewOrigin (._ ⊢> (B <⁰))   (r₀⁰ f)     = go (B <⊢ _)               f  (r₀⁰ [])
    viewOrigin (._ ⊢> (B <⁰))   (r¹₁ f)     = go ((B <⁰) <¹ <⊢ _)       f  (r¹₁ [])
    viewOrigin (._ ⊢> (B <⁰))   (r₁¹ f)     = go (₁> (B <⁰) <⊢ _)       f  (r₁¹ [])
    viewOrigin (._ ⊢> (B <⁰))   (r⇒⊗ f)     = go (_ ⊢> _ ⇒> (B <⁰))     f  (r⇒⊗ [])
    viewOrigin (._ ⊢> (B <⁰))   (r⇐⊗ f)     = go (_ ⊢> (B <⁰) <⇐ _)     f  (r⇐⊗ [])
