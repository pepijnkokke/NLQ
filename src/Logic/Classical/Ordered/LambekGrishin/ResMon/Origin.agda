    -- cases for (₀ , ⁰) and (₁ , ¹)
    viewOrigin (._ ⊢> [])       (r¹₁ f)     = go ([] <¹ <⊢ _) f (r¹₁ [])
    viewOrigin (._ ⊢> [])       (r₁¹ f)     = go (₁> [] <⊢ _) f (r₁¹ [])
