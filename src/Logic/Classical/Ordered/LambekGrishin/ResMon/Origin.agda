    viewOrigin ((A ⊗> B) <⊢ ._) (r◇□ f)     = go ((◇> (A ⊗> B)) <⊢ _)   f  (r◇□ [])
    viewOrigin ((A ⇛> B) <⊢ ._) (r◇□ f)     = go ((◇> (A ⇛> B)) <⊢ _)   f  (r◇□ [])
    viewOrigin ((A ⇚> B) <⊢ ._) (r◇□ f)     = go ((◇> (A ⇚> B)) <⊢ _)   f  (r◇□ [])
    viewOrigin ((A <⊗ B) <⊢ ._) (r◇□ f)     = go ((◇> (A <⊗ B)) <⊢ _)   f  (r◇□ [])
    viewOrigin ((A <⇛ B) <⊢ ._) (r◇□ f)     = go ((◇> (A <⇛ B)) <⊢ _)   f  (r◇□ [])
