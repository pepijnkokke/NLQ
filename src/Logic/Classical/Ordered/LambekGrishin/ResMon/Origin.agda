    private
      go : ∀ {I J B}
         → (I⁻ : Polarised - I) (f : LG I [ B ¹ ]ᴶ)
         → {J⁻ : Polarised - J} (g : ∀ {G} → LG I [ G ]ᴶ ⋯ J [ G ]ᴶ)
         → Origin J⁻ (g [ f ]ᴰ)
      go I⁻ f {J⁻} g with viewOrigin I⁻ f
      ... | origin h f′ pr = origin h (g < f′ >ᴰ) pr′
        where
          pr′ : g [ f ]ᴰ ≡ (g < f′ >ᴰ) [ m¹ h ]ᴰ
          pr′ rewrite <>ᴰ-def f′ g (m¹ h) = cong (_[_]ᴰ g) pr



