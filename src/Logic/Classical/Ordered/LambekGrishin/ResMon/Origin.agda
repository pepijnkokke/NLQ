    private
      go : ∀ {I J B}
         → (I⁺ : Polarised + I) (f : LG I [ el B ]ᴶ)
         → {J⁺ : Polarised + J} (g : ∀ {G} → LG I [ G ]ᴶ ⋯ J [ G ]ᴶ)
         → Origin J⁺ (g [ f ]ᴰ)
      go I⁺ f {J⁺} g with viewOrigin I⁺ f
      ... | origin f′ pr = origin (g < f′ >ᴰ) pr′
        where
          pr′ : g [ f ]ᴰ ≡ (g < f′ >ᴰ) [ ax ]ᴰ
          pr′ rewrite <>ᴰ-def f′ g ax = cong (_[_]ᴰ g) pr


